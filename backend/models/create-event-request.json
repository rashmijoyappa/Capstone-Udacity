{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "event",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 3
    },
    "dueDate": {
      "type": "string"
    },
    "done": {
      "type": "boolean"
    },
    "attachmentUrl": {
      "type": "string"
    }
  },
  "required": ["name", "dueDate"],
  "additionalProperties": false
}
